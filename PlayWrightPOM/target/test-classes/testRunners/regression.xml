<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Open Cart Test Regression PW Suite" thread-count="2" parallel="tests" verbose="4">

	<listeners>
		<listener class-name="com.qa.opencart.listeners.ExtentReportListener"></listener>
	</listeners>

	<test name="Open Cart Home Page Test_safari">

		<classes>
			<class name="com.qa.opencart.test.HomePageTest" />
		</classes>
	</test>

	<test name="Open Cart Login Page Test_safari">

		<classes>
			<class name="com.qa.opencart.test.LoginPageTest" />
		</classes>
	</test>


</suite>